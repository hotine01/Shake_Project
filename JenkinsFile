pipeline {
    agent any

    stages {
        stage('Test') {
            steps {
                   echo "Test"
                }
            }
        stage('Build') {
            steps {
		sh 'mvn package -DskipTests'
		    sh 'docker build -t="hotine01/Shake_Project:latest" .'
                }
            }
        stage('Deploy') {
            steps {
		sh 'docker push  hotine01/Shake_Project:latest'
            }
        }
    }
}
